{
  "name": "geocoder",
  "version": "0.1.1",
  "description": "Angular2 Google Map`",
  "license": "MIT",
  "main": "dist/geocoder.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "set NODE_ENV=dev && webpack-dev-server --quiet --port 9001 --content-base app --config app/webpack.config --open",
    "lint": "tslint 'src/**/*.ts' 'app/**/*.ts'",
    "clean": "rimraf dist",
    "build": "npm-run-all --serial clean build:umd build:app",
    "build:umd": "set NODE_ENV=prod && webpack",
    "build:app": "set NODE_ENV=prod && webpack --config app/webpack.config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenhwkim/geocoder.git"
  },
  "author": "Allen Kim<allenhwkim@gmail.com>",
  "bugs": {
    "url": "https://github.com/allenhwkim/geocoder/issues"
  },
  "homepage": "https://github.com/allenhwkim/geocoder#readme",
  "keywords": [
    "google",
    "map"
  ],
  "devDependencies": {
    "@types/googlemaps": "^3.26.4",
    "@types/node": "^7.0.8",
    "codelyzer": "^2.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.27.3",
    "fetch-jsonp": "^1.0.6",
    "forever": "^0.15.3",
    "fs-extra": "^2.0.0",
    "ng2-utils": "^0.6.1",
    "npm-run-all": "^4.0.2",
    "qwest": "^4.4.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.2.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.14.1",
    "ts-loader": "^2.0.1",
    "tslint": "^4.5.1",
    "typescript": "2.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webtest": "^0.3.8",
    "zone.js": "^0.8.0"
  }
}
